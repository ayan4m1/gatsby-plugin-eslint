  "license": "MIT",
  "author": "Mong-Kuen Sun <mongkuen.sun@gmail.com>",
  "contributors": [
    "ayan4m1 <andrew@bulletlogic.com>"
  ],
  "homepage": "https://github.com/ayan4m1/gatsby-plugin-eslint",
  "bugs": {
